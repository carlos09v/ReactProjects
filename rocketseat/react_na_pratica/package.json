    "3": "npm i lucide-react",
    "4": "npm i tailwind-variants tailwind-merge @radix-ui/react-select @radix-ui/react-dialog",
    "5": "npm i json-server -D",
    "6": "npm i @tanstack/react-query",
    "7": "npm i react-router-dom localforage match-sorter sort-by",
